{
  "name": "maius",
  "version": "0.0.3",
  "description": "A framework for Node.js",
  "main": "./dist/maius.js",
  "scripts": {
    "test": "jest --coverage",
    "dist": "node_modules/.bin/gulp dist:prd",
    "dev": "DEBUG=maius:* node_modules/.bin/gulp dev",
    "dev:plugin": "DEBUG=maius:Plugin* node_modules/.bin/gulp dev",
    "preversion": "npm run test && npm run dist",
    "version": "npm run dist && git add --all dist",
    "postversion": "npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maiusjs/maius.git"
  },
  "keywords": [
    "koa",
    "koa2",
    "framework",
    "maius",
    "maiusjs"
  ],
  "author": "pspgbhu<brotherchun001@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maiusjs/maius/issues"
  },
  "homepage": "https://github.com/maiusjs/maius#readme",
  "dependencies": {
    "debug": "^3.1.0",
    "ejs": "^2.6.1",
    "is-generator-function": "^1.0.7",
    "kcors": "^2.2.1",
    "koa": "^2.5.1",
    "koa-bodyparser": "^4.2.1",
    "koa-convert": "^1.2.0",
    "koa-morgan": "^1.0.1",
    "koa-router": "^7.4.0",
    "koa-static": "^4.0.3",
    "koa-views": "^6.1.4",
    "lodash.merge": "^4.6.1",
    "log4js": "^2.8.0",
    "methods": "^1.1.2",
    "nunjucks": "^3.1.3"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/debug": "0.0.30",
    "@types/ejs": "^2.6.0",
    "@types/jest": "^23.0.0",
    "@types/koa": "^2.0.45",
    "@types/koa-json": "^2.0.18",
    "@types/koa-router": "^7.0.27",
    "@types/koa-static": "^4.0.0",
    "@types/koa-views": "^2.0.3",
    "@types/lodash.merge": "^4.6.4",
    "@types/node": "^9.6.6",
    "chalk": "^2.4.1",
    "del": "^3.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.11.0",
    "gulp": "^3.9.1",
    "gulp-cli": "^2.0.1",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-typescript": "^4.0.2",
    "jest": "^23.1.0",
    "run-sequence": "^2.2.1",
    "ts-jest": "^22.4.6",
    "ts-lint": "^4.5.1",
    "ts-node": "^6.1.0",
    "tslint": "^5.10.0",
    "tslint-config-airbnb": "^5.9.2",
    "typescript": "^2.9.1"
  },
  "jest": {
    "coverageDirectory": "./test/coverage/",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testURL": "http://localhost/",
    "testRegex": "\\.test.ts$",

    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
