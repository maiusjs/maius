{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-airbnb"
  ],
  "jsRules": {},
  "rules": {
    "ter-arrow-parens": [true, "as-needed"],
    "import-name": false,
    "variable-name": [
      true,
      "allow-pascal-case"
    ],
    "array-type": [
      true,
      "array"
    ]
  },
  "rulesDirectory": []
}
